{
  "name": "yusronarif/laravel-core",
  "description": "Laravel Custom Core",
  "keywords": ["php", "laravel", "library", "much", "yusron", "arif", "yusronarif"],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Much Yusron Arif",
      "email": "yusron.arif4@gmail.com",
      "homepage": "https://yusronarif.github.io",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.0",
    "ext-json": "*",
    "composer-runtime-api": "^2.0",
    "illuminate/auth": "^8.40",
    "illuminate/container": "^8.40",
    "illuminate/contracts": "^8.40",
    "illuminate/database": "^8.40",
    "illuminate/support": "^8.40",
    "nesbot/carbon": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0|^9.0"
  },
  "autoload": {
    "psr-4": {
      "Yusronarif\\Core\\": "src/"
    },
    "files": [
      "helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Yusronarif\\Core\\Test\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "3.x-dev"
    },
    "laravel": {
      "providers": [
        "Yusronarif\\Core\\CoreServiceProvider"
      ],
      "aliases": {}
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
